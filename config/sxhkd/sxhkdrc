# refresh sxhkd
super + r
    pkill -SIGUSR1 sxhkd

# Show all commands
super + d
    rofi -show run 

# rofi emoji
super + e
    rofi -show emoji -modi emoji 

# show all apps rofi
super + space 
    rofi -show drun 

# Terminal
super + Return
    alacritty

# Firefox
super + f
    firefox

# Thunar
super + t
    thunar

# screenshot area and copy and save and preview 
super + Print
    flameshot gui
